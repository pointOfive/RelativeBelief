      "cell_type": "code",
      "source": [
        "start_time = time.time()\n",
        "\n",
        "use = 1000\n",
        "theta_values = trace.posterior['theta'].values.ravel()[:use]\n",
        "xu_values = trace.posterior['xu_'].values\n",
        "\n",
        "# Expand dimensions to align for broadcasting\n",
        "theta_expanded = np.expand_dims(theta_values, axis=(0, 1, 2))\n",
        "\n",
        "# Calculate marginal likelihoods using broadcasting\n",
        "marginal_likelihoods2 = bernoulli_logp_np(theta_expanded, xu_values[..., np.newaxis])\n",
        "\n",
        "# Sum the log likelihoods along the appropriate axis and then take the mean\n",
        "marginal_likelihoods2_sum = marginal_likelihoods2.sum(axis=2)\n",
        "marginal_likelihoods2_mean = np.mean(np.exp(marginal_likelihoods2_sum), axis=-1)\n",
        "marginal_likelihoods2_ = np.log(marginal_likelihoods2_mean.ravel())\n",
        "\n",
        "# Calculate bias in favor of the marginal likelihood\n",
        "likelihood_prior_pd2_values = trace.posterior['likelihood_prior_pd2'].values.ravel()\n",
        "bias_for_ml2 = (likelihood_prior_pd2_values - marginal_likelihoods2_) >= 0\n",
        "\n",
        "# Calculate cumulative mean of bias\n",
        "bias_for_ml_mci2 = bias_for_ml2.cumsum() / np.arange(1, bias_for_ml2.shape[0] + 1)\n",
        "\n",
        "# Plotting the results\n",
        "plt.plot(bias_forupper_mci[burn:], label='Bias In Favor Upper (Analytical)')\n",
        "plt.plot(bias_for_ml_mci2[burn:], label='Bias In Favor Upper (Simulation OLD)')\n",
        "plt.xlabel(\"Iterations\")\n",
        "plt.ylabel(\"Bias For Null Hypothesis\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "# Calculate elapsed time\n",
        "end_time = time.time()\n",
        "elapsed_time = end_time - start_time\n",
        "\n",
        "print(f\"Cell execution time: {elapsed_time:.2f} seconds\")"
      ],
      "metadata": {
