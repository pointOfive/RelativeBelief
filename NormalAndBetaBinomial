      "source": [
        "# marginal likelihoods with for loop:\n",
        "start_time = time.time()\n",
        "marginal_likelihoods = np.zeros(list(trace.posterior['x'].values.shape) + [use])\n",
        "\n",
        "# Getting likelihoods for each x_ under each theta_\n",
        "for i, m in enumerate(trace.posterior['theta'].values.ravel()[:use]):\n",
        "    marginal_likelihoods[:, :, :, i] = bernoulli_logp_np(m, trace.posterior['x'].values)\n",
        "\n",
        "# Summing the log likelihoods and then taking the average over the parameter samples\n",
        "marginal_likelihoods_ = np.log(np.exp(marginal_likelihoods.sum(axis=2)).mean(axis=-1).ravel())\n",
        "\n",
        "\n",
        "bias_against_ml = (trace.posterior['likelihood_prior_pd'].values.ravel() - marginal_likelihoods_) <= 0  # Use a small tolerance for floating-point comparison\n",
        "bias_against_ml_mci = bias_against_ml.cumsum() / np.arange(1, bias_against_ml.shape[0] + 1)\n",
        "plt.plot( bias_against_mci[burn:], label='Bias Against (Analytical)')\n",
        "plt.plot(bias_against_ml_mci[burn:], label='Bias Against (Simulation OLD)')\n",
        "plt.xlabel(\"Iterations\")\n",
        "plt.ylabel(\"Bias Against Null Hypothesis\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "end_time = time.time()\n",
        "\n",
        "# Calculate elapsed time\n",
        "elapsed_time = end_time - start_time\n",
        "\n",
        "print(f\"Cell execution time: {elapsed_time:.2f} seconds\")\n",
        "\n"
