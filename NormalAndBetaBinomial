        "# marginal likelihoods with for loop:\n",
        "start_time = time.time()\n",
        "\n",
        "marginal_likelihoods1 = np.zeros(list(trace.posterior['xl_'].values.shape) + [use])\n",
        "\n",
        "# Getting likelihoods for each x_ under each theta_\n",
        "for i, m in enumerate(trace.posterior['theta'].values.ravel()[:use]):\n",
        "    marginal_likelihoods1[:, :, :, i] = bernoulli_logp_np(m, trace.posterior['xl_'].values)\n",
        "\n",
        "# Summing the log likelihoods and then taking the average over the parameter samples\n",
        "marginal_likelihoods1_ = np.log(np.exp(marginal_likelihoods1.sum(axis=2)).mean(axis=-1).ravel())\n",
        "\n",
        "\n",
        "bias_for_ml1 = (trace.posterior['likelihood_prior_pd1'].values.ravel() - marginal_likelihoods1_) >= 0  # Use a small tolerance for floating-point comparison\n",
        "bias_for_ml_mci1 = bias_for_ml1.cumsum() / np.arange(1, bias_for_ml1.shape[0] + 1)\n",
        "plt.plot( bias_forlower_mci[burn:], label='Bias In Favor Lower (Analytical)')\n",
        "plt.plot(bias_for_ml_mci1[burn:], label='Bias In Favor Lower (Simulation OLD)')\n",
        "plt.xlabel(\"Iterations\")\n",
        "plt.ylabel(\"Bias For Null Hypothesis\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "end_time = time.time()\n",
        "\n",
        "# Calculate elapsed time\n",
        "elapsed_time = end_time - start_time\n",
        "\n",
        "print(f\"Cell execution time: {elapsed_time:.2f} seconds\")"
