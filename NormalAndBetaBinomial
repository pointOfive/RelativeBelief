        "id": "lK93hedWS4n2"
      },
      "outputs": [],
      "source": [
        "import numpy as np\n",
        "import pymc as pm\n",
        "import arviz as az\n",
        "import matplotlib.pyplot as plt\n",
        "from scipy.ndimage import gaussian_filter1d\n",
        "from scipy.special import gammaln\n",
        "from scipy import stats\n",
        "import time"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def normal_logp(x, mu, sigma):\n",
        "    return -0.5 * ((x - mu) / sigma) ** 2 - pm.math.log(sigma) - 0.5*pm.math.log(2*np.pi)\n",
        "def normal_logp_np(x, mu, sigma):\n",
        "    return -0.5 * ((x - mu) / sigma) ** 2 - np.log(sigma) - 0.5*np.log(2*np.pi)"
      ],
      "metadata": {
        "id": "IFFjxliAS-I4"
      },
      "execution_count": 16,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": [
        "# Hypothesis"
      ],
      "metadata": {
        "id": "j4Flz-1rGTj6"
      }
    },
    {
      "cell_type": "markdown",
      "source": [
        "## Bias against"
      ],
      "metadata": {
        "id": "6TRPvHY_GxSP"
      }
    },
    {
      "cell_type": "markdown",
      "source": [
        "Model"
      ],
      "metadata": {
        "id": "VKLOzm6WGhW5"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "mu_prior_mean = 0\n",
        "mu_prior_sigma = 1\n",
        "n=50\n",
        "mu_0 = 0\n",
        "with pm.Model() as m:\n",
        "\n",
        "    # traditional analysis\n",
        "    # mu = pm.Normal('mu', mu=mu_prior_mean, sigma=mu_prior_sigma, shape=1)\n",
        "    # y = pm.Normal('y', mu=mu, sigma=1, observed=x)\n",
        "\n",
        "    # \"empty model\" MCMC sample over prior and prior predictive data\n",
        "    mu_ = pm.Normal('mu_', mu=mu_prior_mean, sigma=mu_prior_sigma, shape=1)\n",
        "    x_ = pm.Normal('x_', mu=mu_0, sigma=1, shape=n)\n",
        "\n",
        "    # prior predictive data density evaluations\n",
        "    prior_prior_pd = pm.Deterministic(\"prior_prior_pd\", normal_logp(mu_0,0,1))\n",
        "\n",
        "    posterior_var = 1/(1/mu_prior_sigma**2 + n/1)\n",
        "    posterior_mean = (mu_prior_mean/mu_prior_sigma**2 + x_.sum()/1)*posterior_var\n",
        "    posterior_prior_pd = pm.Deterministic(\"posterior_prior_pd\",\n",
        "                                          normal_logp(mu_0, posterior_mean, posterior_var**0.5))\n",
        "    logBR_prior_pd = pm.Deterministic(\"logBR_prior_pd\", posterior_prior_pd-prior_prior_pd) # pm.math.exp()\n",
        "\n",
        "    likelihood_prior_pd = pm.Deterministic(\"likelihood_prior_pd\", normal_logp(x_,mu_0,1).sum())\n",
        "\n",
        "with m:\n",
        "    idata = pm.sample(draws=1000)"
      ],
      "metadata": {
